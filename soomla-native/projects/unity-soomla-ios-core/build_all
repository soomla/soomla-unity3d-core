#!/bin/sh
set -e
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
cd $DIR

rm -rf build

xcodebuild OTHER_CFLAGS="-fembed-bitcode" -configuration Release -sdk iphoneos -project UnitySoomlaiOSCore.xcodeproj -target UnitySoomlaiOSCore clean build CREATING_UNIVERSAL_DIR=$PWD/build/release

cd - > /dev/null
